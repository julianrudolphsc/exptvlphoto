<% include ./partials/header%>

    <div class="container">
        <div class="row">
            <div class="jumbotron text-center">
                <h1>Travel Photo App!</h1>
                <p>Photos from all over the world!</p>
            </div>
        </div>
        <%photos.forEach(function(photo){ %>
            <div>
                <h2><%= photo.title%></h2>
                <img src="<%= photo.image%>"></img>
            </div>
        <% }) %>
    </div>

<% include ./partials/footer%>